<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="160.0" prefWidth="480.0" styleClass="modal-root" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cashiepay.DAO.EditMfoPapController">

    <stylesheets>
        <URL value="@/cashiepay/style/addnewparticular.css" />
    </stylesheets>

    <children>
        <VBox spacing="15" AnchorPane.bottomAnchor="20" AnchorPane.leftAnchor="20" AnchorPane.rightAnchor="20" AnchorPane.topAnchor="20">

            <Label styleClass="modal-title" text="Edit Fund" />

            <VBox spacing="10">
                <VBox spacing="5">
                    <Label styleClass="field-label" text="Fund Name:" />
                    <TextField fx:id="nameField" promptText="Enter fund name" styleClass="input-field" />
                </VBox>
            </VBox>

            <HBox alignment="CENTER_RIGHT" spacing="10">
                <Button fx:id="cancelBtn" onAction="#cancelAction" styleClass="btn-secondary" text="Cancel" />
                <Button fx:id="saveBtn" onAction="#saveAction" styleClass="btn-primary" text="Save" />
            </HBox>

        </VBox>
    </children>
</AnchorPane>
